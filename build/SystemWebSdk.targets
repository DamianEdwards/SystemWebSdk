<Project>
  <Target Name="PublishFolderCleanup" AfterTargets="Publish">
    <Exec WorkingDirectory="$(PublishDir)" Command="(robocopy .\ .\bin *.dll *.dll.config *.pdb *.xml /mov /is /it) ^&amp; IF %ERRORLEVEL% LEQ 4 exit /B 0" />
    <Exec WorkingDirectory="$(PublishDir)" Command="rmdir refs /s /q" />
    <Exec WorkingDirectory="$(PublishDir)" Command="del *.*.config" />
    <Exec WorkingDirectory="$(PublishDir)" Command="del app.config" />
  </Target>
</Project>
